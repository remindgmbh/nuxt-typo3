{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "@nuxt/kit"
    ],
    "baseUrl": ".",
    "paths": {
      "#app": [
        "./node_modules/nuxt/dist/app"
      ],
      "#head": [
        "./node_modules/nuxt/dist/head/runtime"
      ],
    }
  },
  "include": [
    "./playground/.nuxt/components.d.ts",
    "./playground/.nuxt/types/schema.d.ts",
    "src"
  ]
}