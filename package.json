    "nuxt": "^3.1.2",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.3",
    "stylelint": "^14.16.1",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4",
    "vue-tsc": "^1.0.24"
