{
    "name": "@remindgmbh/nuxt-typo3",
    "version": "0.0.0-development",
    "author": "David Mellen <d.mellen@remind.de>",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/remindgmbh/nuxt-typo3.git"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types.d.ts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "npm run dev:prepare && nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "lint": "npm run lint:script && npm run lint:style",
        "lint:script": "eslint --ext \".js,.ts,.vue\" .",
        "lint:style": "stylelint \"**/*.{vue,[s]css}\"",
        "prepack": "npm run dev:prepare && nuxt-module-build build",
        "preview": "nuxi preview playground",
        "static-analysis": "npm run dev:prepare && npm run typecheck && npm run lint",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "typecheck": "nuxi typecheck"
    },
    "dependencies": {
        "@nuxt/kit": "^3.9.3",
        "@remindgmbh/nuxt-logger": "^1.0.3",
        "defu": "^6.1.4",
        "perfect-debounce": "^1.0.0",
        "sass": "^1.70.0",
        "scule": "^1.2.0",
        "vee-validate": "^4.12.4",
        "vue-i18n": "^9.9.0",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@nuxt/module-builder": "^0.5.5",
        "@nuxt/schema": "^3.9.3",
        "@remindgmbh/eslint-config-nuxt": "^1.1.0",
        "@remindgmbh/stylelint-config": "^1.1.1",
        "@testing-library/jest-dom": "^6.3.0",
        "@testing-library/vue": "^8.0.0",
        "@types/node": "^20.11.7",
        "eslint": "^8.56.0",
        "happy-dom": "^12.10.3",
        "nuxt": "^3.9.3",
        "prettier": "^3.2.4",
        "stylelint": "^16.2.0",
        "typescript": "^5.3.3",
        "vitest": "^0.34.6",
        "vue-tsc": "^1.8.27"
    },
    "volta": {
        "node": "20.9.0"
    }
}
