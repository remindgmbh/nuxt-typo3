{
    "name": "@remindgmbh/nuxt-typo3",
    "version": "2.0.2",
    "author": "David Mellen <d.mellen@remind.de>",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "https://github.com/remindgmbh/nuxt-typo3"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "yarn stub && nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:test": "vitest",
        "lint": "yarn lint:prettier && yarn lint:script && yarn lint:style",
        "lint:prettier": "prettier --check .",
        "lint:script": "eslint --ext \".js,.ts,.vue\" .",
        "lint:style": "stylelint \"**/*.{vue,[s]css}\"",
        "prepare": "yarn stub && nuxt-module-build",
        "prepublish": "yarn test && yarn lint",
        "preview": "nuxi preview playground",
        "stub": "nuxt-module-build --stub && nuxi prepare playground",
        "test": "vitest run"
    },
    "dependencies": {
        "@nuxt/kit": "^3.2.2",
        "@remindgmbh/nuxt-logger": "^1.0.1",
        "defu": "^6.1.2",
        "perfect-debounce": "^0.1.3",
        "sass": "^1.58.3",
        "sass-loader": "^13.2.0",
        "scule": "^1.0.0",
        "swiper": "^9.0.5",
        "vee-validate": "^4.7.4",
        "vue-i18n": "^9.2.2",
        "yup": "^1.0.0"
    },
    "devDependencies": {
        "@nuxt/module-builder": "0.2.0",
        "@remindgmbh/eslint-config-nuxt": "^1.0.3",
        "@remindgmbh/stylelint-config": "^1.0.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/vue": "^6.6.1",
        "eslint": "^8.35.0",
        "jsdom": "^21.1.0",
        "nuxt": "^3.2.2",
        "postcss": "^8.4.21",
        "postcss-html": "^1.5.0",
        "postcss-scss": "^4.0.6",
        "prettier": "^2.8.4",
        "stylelint": "^15.2.0",
        "typescript": "^4.9.5",
        "vite-tsconfig-paths": "^4.0.5",
        "vitest": "^0.28.5",
        "vue-tsc": "^1.1.5"
    },
    "volta": {
        "node": "16.17.0",
        "yarn": "1.22.19"
    }
}
