{
    "name": "@remindgmbh/nuxt-typo3",
    "version": "2.0.2",
    "author": "David Mellen <d.mellen@remind.de>",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "https://github.com/remindgmbh/nuxt-typo3"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "yarn stub && nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:test": "vitest",
        "lint": "yarn lint:prettier && yarn lint:script && yarn lint:style",
        "lint:prettier": "prettier --check .",
        "lint:script": "eslint --ext \".js,.ts,.vue\" .",
        "lint:style": "stylelint \"**/*.{vue,[s]css}\"",
        "postinstall": "yarn --cwd playground",
        "prepare": "yarn stub && nuxt-module-build",
        "prepublishOnly": "yarn typecheck && yarn lint && yarn test",
        "preview": "nuxi preview playground",
        "stub": "nuxt-module-build --stub && nuxi prepare playground",
        "test": "vitest run",
        "typecheck": "nuxi typecheck"
    },
    "dependencies": {
        "@nuxt/kit": "^3.4.2",
        "@remindgmbh/nuxt-logger": "^1.0.1",
        "defu": "^6.1.2",
        "perfect-debounce": "^0.1.3",
        "sass": "^1.62.1",
        "sass-loader": "^13.2.2",
        "scule": "^1.0.0",
        "swiper": "^9.2.4",
        "vee-validate": "^4.8.6",
        "vue-i18n": "^9.2.2",
        "yup": "^1.1.1"
    },
    "devDependencies": {
        "@nuxt/module-builder": "^0.3.0",
        "@remindgmbh/eslint-config-nuxt": "^1.0.4",
        "@remindgmbh/stylelint-config": "^1.0.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/vue": "^7.0.0",
        "@types/node": "^18.16.1",
        "eslint": "^8.39.0",
        "happy-dom": "^9.9.2",
        "nuxt": "^3.4.2",
        "postcss": "^8.4.23",
        "postcss-html": "^1.5.0",
        "postcss-scss": "^4.0.6",
        "prettier": "^2.8.8",
        "rollup-plugin-copy": "^3.4.0",
        "stylelint": "^15.6.0",
        "typescript": "^5.0.4",
        "vitest": "^0.30.1",
        "vue-tsc": "^1.4.4"
    },
    "volta": {
        "node": "18.16.0",
        "yarn": "1.22.19"
    }
}
